{
    "name": "qbit-lang",
    "displayName": "Qbit Language Support",
    "description": "Language support for Qbit",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "qbit",
                "aliases": [
                    "Qbit",
                    "qbit"
                ],
                "extensions": [
                    ".qb",
                    ".qbit"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "qbit",
                "scopeName": "source.qbit",
                "path": "./syntaxes/qbit.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [],
    "devDependencies": {
        "@types/node": "^16.18.126",
        "@types/vscode": "^1.104.0",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
}
